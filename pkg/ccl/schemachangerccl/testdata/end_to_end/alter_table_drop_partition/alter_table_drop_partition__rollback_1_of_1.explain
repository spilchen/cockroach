/* setup */
CREATE TABLE t_range (
  a INT,
  b INT,
  PRIMARY KEY (a, b)
) PARTITION BY RANGE (a) (
  PARTITION p1 VALUES FROM (1) TO (10),
  PARTITION p2 VALUES FROM (10) TO (20),
  PARTITION p3 VALUES FROM (20) TO (30)
);
INSERT INTO t_range VALUES (5, 1), (15, 1), (25, 1);

