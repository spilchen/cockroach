# Test that partitioning is now available in the base binary (no longer requires CCL).

statement ok
CREATE TABLE t (a INT, b INT, c INT, PRIMARY KEY (a, b)) PARTITION BY LIST (a) (
    PARTITION p1 VALUES IN (1),
    PARTITION p2 VALUES IN (2)
)

statement ok
DROP TABLE t

statement ok
CREATE TABLE t (
    a INT PRIMARY KEY, b INT,
    INDEX (b) PARTITION BY LIST (b) (
        PARTITION p1 VALUES IN (1)
    )
)

statement ok
DROP TABLE t
